# Single API key (legacy support)
GEMINI_API_KEY=your_gemini_api_key_here

# Multiple API keys for automatic retry (recommended)
# If quota is exceeded on one key, the system will automatically try the next one
# GEMINI_API_KEY_1=your_first_api_key_here
# GEMINI_API_KEY_2=your_second_api_key_here
# GEMINI_API_KEY_3=your_third_api_key_here
# Add as many as you need: GEMINI_API_KEY_4, GEMINI_API_KEY_5, etc.

PORT=5000
